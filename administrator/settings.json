{
  "version": "1.0.0",
  "startPageCommand": "showContentPanel", // start page - options: showDashboardEditorPanel, showDataRobotsPanel, showContentPanel, showFullSensorsListPanel, showObjectEditorPanel, showComponentEditorPanel, showTemplateEditorPanel

  "serviceUrl": "url_to_web_service_endpoint",
  "basicUrl": "url_to_base_domain",

  // MAP
  "mapConfiguration": {
    "initialExtent": [ 0.0, 0.0, 0.0, 0.0 ],
    "scale": {
      "min": 250000,
      "max": 50
    },
    "scaleList": [ 50, 100, 200, 300, 400, 500, 1000, 1250, 1500, 2000, 3000, 4000, 5000, 10000, 15000, 20000, 25000, 50000, 75000, 100000, 150000, 200000, 250000 ],
    "wmsTileSize": 512,
    "projection": {
      "envelope": {
        "maxX": 0.0,
        "maxY": 0.0,
        "minX": 0.0,
        "minY": 0.0
      },
      "name": "projection_name",
      "definition": "projection_definition"
    },
    "baseMapLayer": {
      "capabilitiesUrl": "", // default
      "endpointVersion": "",
      "endpointType": "",
      "layerName": ""
    },
    "buffer": 50,
    "showBufferCircle": false,
    "layersCapabilitiesUrl": "url_to_layers_capabilities_endpoint",
    "descripeFeatureTypeUrl": "url_to_feature_description_endpoint",
    "showLayersOnStart":  false
  },

  // SEARCH
  "mapSearchConfig": {
    "serverUrl": "url_to_map_search_endpoint", // default
    "searchFieldName": "searchfield",
    "searchSize": 20,
    "singleSearchScale": 400,
    "indexNames": "brond,matrikel,adresser",
    "pointStyle": {
      "type": "Icon",
      "iconSource": "url_to_icon_file",
      "opacity": 0.75,
      "scale": 1,
      "radius": 1,
      "fillColor": "ff0000bf",
      "strokeColor": "ff0000bf",
      "strokeWidth": 1
    },
    "polygonStyle": {
      "fillColor": "ff0000bf",
      "strokeColor": "ff0000bf",
      "strokeWidth": 1,
      "style": "Dash dot"
    },
    "lineStyle": {
      "strokeColor": "ff0000bf",
      "strokeWidth": 1,
      "style": "dashdot"
    }
  },
  "debuggingActive":  false
}
